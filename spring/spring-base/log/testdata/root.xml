<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console"/>
    </Appenders>
    <Loggers>
        <Logger name="spring/spring-base/log_test" level="debug">
            <AppenderRef ref="Console">
                <Filters>
                    <LevelFilter level="info"/>
                </Filters>
            </AppenderRef>
        </Logger>
        <Root level="debug">
            <AppenderRef ref="Console"/>
            <LevelFilter level="info"/>
        </Root>
    </Loggers>
</Configuration>